{
    "name": "vscode-wiki-fs",
    "displayName": "Wiki FileSystem",
    "description": "Access a wiki as workspace folder.",
    "version": "0.0.1",
    "publisher": "ego-digital",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Wiki",
        "REST",
        "API",
        "Workspace",
        "Folder",
        "multi-root ready"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.4",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/egodigital/vscode-wiki-fs.git"
    },
    "readmeFilename": "README.md",
    "license": "LGPL-3.0",
    "bugs": {
        "url": "https://github.com/egodigital/vscode-wiki-fs/issues"
    },
    "homepage": "https://github.com/egodigital/vscode-wiki-fs#readme",
    "dependencies": {
        "fs-extra": "^7.0.0",
        "moment": "^2.22.2",
        "vscode-helpers": "^2.12.0"
    }
}
